<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              xmlns:app="http://schemas.android.com/apk/res-auto"
              android:orientation="vertical"
              android:layout_width="match_parent"
              android:layout_height="match_parent">


    <android.support.design.widget.AppBarLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <android.support.v7.widget.Toolbar
            android:id="@+id/dev_create_records_layout__toolbar"
            style="@style/AppToolbar"
            app:title="Create Dummies"
            />

    </android.support.design.widget.AppBarLayout>

    <Button
        android:id="@+id/dev_create_records_layout__create_dummies"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="create dummies"
        />

    <Button
        android:id="@+id/dev_create_records_layout__delete_database"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="delete database"
        />

</LinearLayout>
